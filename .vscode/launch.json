{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch via NPM",
        "runtimeExecutable": "npm",
        "runtimeArgs": [
          "run-script",
          "dev",
          "new",
          "debug_project"
        ],
        "port": 9229,
        "sourceMaps": true,
        "outFiles": [
          "${workspaceFolder}/**/*.ts"
        ],
        "protocol": "inspector"
      }
    ]
  }
  